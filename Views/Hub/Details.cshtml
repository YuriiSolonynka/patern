@model patern.Models.Hub

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{
    ViewData["Title"] = "Details";
}

<h1>Hub Details</h1>

<div>
    <h4>Hub</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Id</dt>
        <dd class="col-sm-10">@Model.Id</dd>
        <dt class="col-sm-2">Location</dt>
        <dd class="col-sm-10">@Model.Location</dd>
        <dt class="col-sm-2">UserId</dt>
        <dd class="col-sm-10">@Model.UserId</dd>
        <dt class="col-sm-2">SecurityServiceId</dt>
        <dd class="col-sm-10">@Model.SecurityServiceId</dd>
    </dl>
</div>
<p>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary btn-sm">Edit</a>
    <a asp-action="Index" class="btn btn-success btn-sm">Back to List</a>
</p>
